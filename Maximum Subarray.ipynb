    "class Solution(object):\n",
    "    def maxSubArray(self, nums):\n",
    "        fn=0\n",
    "        max_A=nums[0]\n",
    "        \n",
    "        for i in nums:\n",
    "            fn=max([i,fn+i])\n",
    "            max_A=max(max_A,fn)\n",
    "        return max_A"
   ]
  }
